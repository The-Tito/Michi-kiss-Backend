<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <!-- Suprimir warnings especÃ­ficos de Jetty multipart -->
    <logger name="org.eclipse.jetty.server.Request" level="ERROR"/>
    <logger name="org.eclipse.jetty.server.MultiPartFormInputStream" level="ERROR"/>
    <logger name="org.eclipse.jetty.server.MultiParts" level="ERROR"/>
    <logger name="org.eclipse.jetty.util.MultiException" level="ERROR"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>